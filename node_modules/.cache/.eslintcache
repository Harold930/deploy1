    }\n\n    if(Array.isArray(movies)){\n        if(movies.length <= 4){\n            return (\n            <div className={home.contain1}>\n                <div className={home.contain}>\n                    <div>\n                    <button onClick={handleClick} name='popular' >PELÍCULAS MÁS RECIENTES</button>\n                    </div>\n                    <div>\n                    <button onClick={handleClick} name='top-rated'>PELÍCULAS MEJOR CALIFICADAS</button>\n                    </div>\n                    <div>\n                    <button onClick={handleClick} name='reset'>PELÍCULAS MÁS POPULARES</button>\n                    </div>\n                </div>\n            <CardsMovies movies={movies}/>\n        </div>\n            )\n        }else{\n            return (\n                <div>\n                    <div className={home.contain}>\n                        <div>\n                        <button onClick={handleClick} name='popular' >PELÍCULAS MÁS RECIENTES</button>\n                        </div>\n                        <div>\n                        <button onClick={handleClick} name='top-rated'>PELÍCULAS MEJOR CALIFICADAS</button>\n                        </div>\n                        <div>\n                        <button onClick={handleClick} name='reset'>PELÍCULAS MÁS POPULARES</button>\n                        </div>\n                    </div>\n                    <CardsMovies movies={movies}/>\n                </div>\n            )\n        }\n    } else {\n        return(\n          <div className={home.contain1}>\n            <div className={home.contain}>\n                <div>\n                    <button onClick={handleClick} name='popular' >PELÍCULAS MÁS POPULARES</button>\n                </div>\n                <div>\n                    <button onClick={handleClick} name='top-rated'>PELÍCULAS MEJOR CALIFICADAS</button>\n                </div>\n                <div>\n                    <button onClick={handleClick} name='reset'>PELÍCULAS MÁS RECIENTES</button>\n                </div>\n            </div>\n            <h1 className={home.font}>{movies.msg}</h1>\n          </div>\n        )\n    }\n}\n\n\n","/home/harold/Escritorio/AppMovies/client/src/Components/Nav/Nav.js",["99","100"],"\nimport React, { useState } from 'react';\nimport { useDispatch } from 'react-redux';\nimport { NavLink } from 'react-router-dom';\nimport { getMoviesByName } from '../../Redux/actions';\nimport nav from './nav.module.css';\nimport icono from '../../img/lupa.webp';\n\nexport default function Nav(){\n\n    const [name, setName] = useState('');\n    const dispatch = useDispatch();\n\n    function handleChange(e){\n        setName(e.target.value);\n    }\n\n    function handleSearch  (e) {\n        e.preventDefault();\n        dispatch(getMoviesByName(name));\n        setName('');\n    }\n\n    function handleCLick(e){\n        e.preventDefault();\n        dispatch(getMoviesByName(name));\n        setName('')\n    }\n\n    return(\n        <nav className={nav.nav}>\n           <NavLink to = '/'  style={{ textDecoration: 'none' }}>\n                <h1 className={nav.font}>HOME</h1>\n           </NavLink>\n           <h1 className={nav.font}>PELÍCULAS</h1>\n\n           <div>\n                <button onClick={handleCLick} ><img src={icono} className={nav.button}/></button>\n                <input className = {nav.input}type=\"text\" placeholder='Buscar por nombre' value={name}  onChange={handleChange} onKeyPress={(e) => e.key === 'Enter' && handleSearch(e)} />\n           </div>\n        </nav>\n    )\n}\n\n{/* <NavLink to={`/home/pokemons/search?name=${name}`} style={{ textDecoration: 'none' }}><img src={icono} className={nav.button}/></NavLink> */}","/home/harold/Escritorio/AppMovies/client/src/Components/Detail/Detail.js",["101"],"import React, { useEffect } from 'react';\nimport {useDispatch, useSelector} from 'react-redux';\nimport { getDetail } from '../../Redux/actions';\nimport detail from './detail